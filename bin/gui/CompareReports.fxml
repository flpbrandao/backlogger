<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER_RIGHT" prefHeight="588.0" prefWidth="571.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.CompareReportsController">
   <children>
      <AnchorPane prefHeight="565.0" prefWidth="571.0">
         <children>
            <Button fx:id="btPreviousBacklog" layoutX="8.0" layoutY="57.0" mnemonicParsing="false" onAction="#onBtPreviousBacklog" prefHeight="25.0" prefWidth="559.0" text="Previous Backlog" />
            <TableView fx:id="tableView" editable="true" layoutX="10.0" layoutY="96.0" prefHeight="401.0" prefWidth="559.0">
              <columns>
                <TableColumn fx:id="tableColumnNumber" prefWidth="78.0" text="Number" />
                <TableColumn fx:id="tableColumnAssignedTo" prefWidth="104.0" text="Assigned To" />
                  <TableColumn fx:id="tableColumnAssignmentGroup" prefWidth="115.0" text="Queue" />
                  <TableColumn fx:id="tableColumnCreatedOn" prefWidth="89.0" text="Created" />
                  <TableColumn fx:id="tableColumnUpdatedOn" prefWidth="70.0" text="Updated" />
                  <TableColumn fx:id="tableColumnStatus" prefWidth="88.0" text="Status" />
              </columns>
            </TableView>
            <Label alignment="CENTER" layoutX="30.0" layoutY="7.0" prefHeight="44.0" prefWidth="517.0" text="Selecione dois arquivos para comparação. Eles ficam na pasta da aplicação, &quot;data&quot;. Utilize os arquivos que tem o final &quot;FINAL-CSV-REPORT.&quot;" textAlignment="CENTER" wrapText="true" />
         </children></AnchorPane>
   </children>
</VBox>
