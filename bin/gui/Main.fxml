<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="479.0" prefWidth="568.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MainController">
   <children>
      <TextField fx:id="txtPathFile" alignment="BOTTOM_CENTER" layoutX="33.0" layoutY="56.0" prefHeight="25.0" prefWidth="345.0" promptText="Para arquivo na raiz, digite apenas o nome " />
      <Button fx:id="btOk" layoutX="401.0" layoutY="54.0" mnemonicParsing="false" onAction="#onBtOkAction" prefHeight="29.0" prefWidth="134.0" text="Ler arquivo CSV" />
      <TextField fx:id="txtExcludeFile" alignment="BOTTOM_CENTER" layoutX="33.0" layoutY="93.0" prefHeight="25.0" prefWidth="345.0" promptText="Lista de tickets a serem excluídos do relatório de backlog" />
      <Button fx:id="btExclude" layoutX="401.0" layoutY="91.0" mnemonicParsing="false" onAction="#onBtExcludeAction" prefHeight="29.0" prefWidth="134.0" text="Arquivos a excluir" />
      <Label prefHeight="30.0" prefWidth="237.0" text="Backlogger - gerador de reports">
         <padding>
            <Insets left="10.0" top="10.0" />
         </padding>
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button fx:id="btGenerate" layoutX="206.0" layoutY="133.0" mnemonicParsing="false" onAction="#onBtGenerateAction" prefHeight="30.0" prefWidth="125.0" text="Gerar report" />
      <TableView fx:id="tableViewTicket" layoutX="9.0" layoutY="179.0" prefHeight="293.0" prefWidth="552.0">
        <columns>
          <TableColumn fx:id="tableColumnNumber" prefWidth="75.0" text="C1" />
          <TableColumn fx:id="tableColumnAssignedTo" prefWidth="75.0" text="C2" />
            <TableColumn fx:id="tableColumnAssignmentGroup" prefWidth="75.0" text="C2" />
            <TableColumn fx:id="tableColumnCreatedOn" prefWidth="75.0" text="C2" />
            <TableColumn fx:id="tableColumnUpdatedOn" prefWidth="75.0" text="C2" />
            <TableColumn fx:id="tableColumnStatus" prefWidth="75.0" text="C2" />
            <TableColumn fx:id="tableColumnCompleted" prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
